fun very_slow_exp(a, n) {
    var r = 1;
    for (var i = 0; i < n; i = i + 1) r = r * a;
    return r;
}

print very_slow_exp(3, 3);

fun add(a, b) {
    return a + b;
}

if (add(4,5) == add(6,3)) {
    print "Equal";
} else {
    print "Inequal";
}

fun x(a, b) {
    return a + b;
}

print x(
    x(5, 5), 
    x(5, 5)
);
print x(
    x("Hello", ", "), 
    x("World", "!")
);
