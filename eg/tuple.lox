{ 
    class Tuple {
        init(x, y) {
           this.x = x;
           this.y = y;
        }

        first() {
           return this.x;
        }

        second() {
           return this.y;
        }

        sum() {
           return this.x + this.y;
        }

        prod() {
           return this.x * this.y;
        }
    }

    {
        var t = Tuple(3,4);
        print t.first();
        print t.second();
        print t.sum();
        print t.prod();
    }
}
